{
  "manifest_version": 3,
  "name": "Nova Ixl Extension",
  "version": "1.0",
  "description": "This extension allows you to use Nova in Ixl, to skip individual questions.",
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "https://raw.githubusercontent.com/Cpmjaguar1234/nova/refs/heads/main/nova%20logo%20png.png",
      "32": "https://raw.githubusercontent.com/Cpmjaguar1234/nova/refs/heads/main/nova%20logo%20png.png",
      "48": "https://raw.githubusercontent.com/Cpmjaguar1234/nova/refs/heads/main/nova%20logo%20png.png",
      "128": "https://raw.githubusercontent.com/Cpmjaguar1234/nova/refs/heads/main/nova%20logo%20png.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [{
    "resources": ["popup.js"],
    "matches": ["<all_urls>"]
  }]
}